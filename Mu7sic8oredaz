    Name = "IDØ§Ø¯Ø®Ø§Ù„",
    Description = "IDÙÙ‚Ø·",
    PlaceholderText = "07207502:Ù…Ø«Ù„ IDØ§ÙƒØªØ¨ Ù‡Ù†Ø§",
    Callback = function(value)
        
        local id = tostring(value):match("%d+")
        if id then
            LastSongID = id
            PlayMusic(id)
        else
            warn("Ø§Ù„Ø±Ø¬Ø§Ø¡ Ø¥Ø¯Ø®Ø§Ù„ ID ØµØ­ÙŠØ­ (Ø£Ø±Ù‚Ø§Ù… ÙÙ‚Ø·)")
        end
    end
})

MusicTab:AddButton({
    "ØªØ´ØºÙŠÙ„",
    function()
        if LastSongID then
            PlayMusic(LastSongID)
        else
            warn("Ù„Ø§ ÙŠÙˆØ¬Ø¯ ID Ù…Ø­ÙÙˆØ¸. Ø£Ø¯Ø®Ù„ ID Ø£ÙˆÙ„Ø§Ù‹ ÙÙŠ Ø§Ù„Ù€ TextBox.")
