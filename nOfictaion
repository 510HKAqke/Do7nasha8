	Position = UDim2.new(0.5, -130, 0, 30)  
})  
tween:Play()  

delay(3, function()  
	if gui and gui.Parent then  
		local fade = TweenService:Create(frame, TweenInfo.new(0.5), { BackgroundTransparency = 1 })  
		fade:Play()  
		fade.Completed:Wait()  
		gui:Destroy()  
	end  
